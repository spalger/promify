    "es6"
  ],
  "author": "Spencer Alger <email@spalger.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spalger/promify/issues"
  },
  "homepage": "https://github.com/spalger/promify#readme",
  "devDependencies": {
    "babel": "^5.5.7",
    "chai": "^3.0.0",
    "mocha": "^2.2.5"
  }
}
